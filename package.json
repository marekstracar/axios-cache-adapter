{
    "name": "@marekstracar/axios-cache-adapter",
    "version": "2.8.2",
    "description": "Caching adapter for stable version of Axios (1.x)",
    "homepage": "https://github.com/marekstracar/axios-cache-adapter",
    "author": {
        "name": "Marek Stracar",
        "email": "marek851@protonmail.com",
        "url": "https://github.com/marekstracar"
    },
    "contributors": [
        {
            "name": "Carl Ogren",
            "email": "rascarlito@gmail.com",
            "url": "https://github.com/RasCarlito"
        }
    ],
    "files": [
        "/dist",
        "/src",
        "/axios-cache-adapter.d.ts"
    ],
    "main": "dist/cache.node.js",
    "browser": "dist/cache.js",
    "types": "axios-cache-adapter.d.ts",
    "runkitExampleFilename": "examples/basic.js",
    "exclude": [
        "node_modules",
        "karma.conf.cjs",
        "webpack.config.cjs"
    ],
    "keywords": [
        "axios",
        "cache",
        "xhr",
        "request"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "lint": "eslint",
        "lint-fix": "eslint --fix src/**/*.js test/**/*.js",
        "build": "npm run clean && webpack --config webpack.config.js && NODE_ENV=production webpack --config webpack.config.js && NODE_BUILD_FOR=node webpack --config webpack.config.js && NODE_ENV=production NODE_BUILD_FOR=node webpack --config webpack.config.js",
        "test": "NODE_ENV=test karma start karma.conf.js",
        "watch": "NODE_WATCH=true npm test"
    },
    "repository": "https://github.com/marekstracar/axios-cache-adapter",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/marekstracar/axios-cache-adapter/issues"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@marekstracar/axios-cache-adapter": "^2.8.2",
        "assert": "^2.0.0",
        "axios": "^0.20.0 || ^1.0.0",
        "babel-loader": "^8.0.4",
        "eslint": "^8.32.0",
        "html-webpack-plugin": "^4.5.2",
        "husky": "^8.0.3",
        "istanbul-instrumenter-loader": "^2.0.0",
        "karma": "^6.4.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^1.1.1",
        "karma-coverage-istanbul-reporter": "^1.3.0",
        "karma-mocha": "^2.0.1",
        "karma-mocha-reporter": "^2.2.5",
        "karma-sourcemap-loader": "^0.3.8",
        "karma-webpack": "^4.0.2",
        "localforage": "^1.7.2",
        "localforage-memoryStorageDriver": "^0.9.2",
        "lodash": "^4.17.11",
        "mocha": "^10.2.0",
        "mockdate": "^2.0.2",
        "puppeteer": "^19.5.2",
        "redis-mock": "^0.49.0",
        "rimraf": "^4.1.1",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "@tusbar/cache-control": "^0.6.1",
        "md5": "^2.2.1"
    },
    "peerDependencies": {
        "axios": "^0.20.0 || ^1.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    }
}
